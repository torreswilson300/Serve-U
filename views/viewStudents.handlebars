        <button><a href="/orgs/remove/{{id}}" class="a" > Remove Student </a></button>
        {{!-- <button><a href="/students/v" class="a" >  View </a>  </button> --}}
    </ul>

    <script>
        function approved(x){
            x = true
            alert("isApproved should be true" + {{x}});
            return x;
        }
    </script>

    </div>
    {{/each}}
