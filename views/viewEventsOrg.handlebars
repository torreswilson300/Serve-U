<section>
    <h1>Manage Events</h1>
    {{#each o}}
     <form action="/orgs/viewEvents" method="POST">
    <div class = "org" >
   
      <h2>Title: {{title}}</h2>  
      <p> Host: {{host}}</p>
      <p>Organization: {{org}}</p>
      <p>Date of Event: {{ date}}</p>
      <p>Start: {{ start }}</p>
      <p>End: {{ end }}</p>
      <p>Description: {{description}}</p>
     <strong> <p>Hours Received: {{hoursReceived}}</p></strong>
     <ul>
    <li> <button><a href="/orgs/delete/{{id}}" class="a" > Delete Event </a></button></li>
    <li> <button><a href="/orgs/editEvent/{{id}}" class="a" > Edit Event </a></button></li></ul>
     
    </div>
    {{/each}}
    </form>
</section>