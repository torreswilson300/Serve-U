<section>
    <h1>Manage Events</h1>
    {{#each o}}
     <form action="/orgs/viewEvents" method="POST"></form>
    <div class = "org" >
        <center>
      <h2>Title: {{title}}</h2>  
      <p> Host: {{host}}</p>
      <p>Organization: {{org}}</p>
      <p>Date of Event: {{ date}}</p>
      <p>Start: {{ start }}</p>
      <p>End: {{ end }}</p>
      <p>Description: {{description}}</p>
     <strong> <p>Hours Received: {{hoursReceived}}</p></strong>
     <button><a href="/orgs/delete/{{id}}" class="a" > Delete Event </a></button>
      </center>
    </div>
    {{/each}}
</section>